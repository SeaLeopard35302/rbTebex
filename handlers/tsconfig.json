{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "./build",
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./handlers",
    "paths": {
      "handler": ["./build/*"]
    }
  }
}
